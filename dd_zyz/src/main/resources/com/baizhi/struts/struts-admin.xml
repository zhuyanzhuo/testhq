<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN" "struts-2.1.7.dtd" >
<struts>
	<package name="admin" extends="struts-default" namespace="/admin">
		<action name="login" class="com.baizhi.action.AdminAction" method="login">
			<result>/back/main/main.jsp</result>
			<result name="error">/back/login.jsp</result>
		</action>
		
		<action name="code" class="com.baizhi.action.AdminAction" method="code">
			<result>/back/login.jsp</result>
		</action>
		
		<action name="exit" class="com.baizhi.action.AdminAction" method="exit">
			<result type="redirect">/back/login.jsp</result>
		</action>
	</package>
</struts>
